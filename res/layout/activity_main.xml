<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.activityintentfilter.MainActivity" >

    <Button
        android:id="@+id/bt_multi_matched"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="多个 category 完全匹配" />

    <Button
        android:id="@+id/bt_single_matched"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="单个 category 完全匹配" />

    <Button
        android:id="@+id/bt_no_action"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="缺少  action" />

    <Button
        android:id="@+id/bt_no_category"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="缺少  category" />

    <Button
        android:id="@+id/bt_no_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="缺少  data" />

    <Button
        android:id="@+id/bt_no_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="data 缺少  type" />

    <Button
        android:id="@+id/bt_browser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="启动浏览器" />

</LinearLayout>